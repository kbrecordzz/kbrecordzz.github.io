{
	if (dialog !== last_dialog) now_char = 0;

	if (now_char < dialog.length)
	{
		now_char += 2;
		document.getElementById("dialog_text").innerHTML = dialog.substring(0, now_char);
	}

	last_dialog = dialog;
}

