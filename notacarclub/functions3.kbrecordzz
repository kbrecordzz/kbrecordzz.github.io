				ts_end(CUT_CUTSCENE_BLACKINTRO);
			}
			else
			{
				sound_carstart.play();
			}
		}
	}

	// RACE ONLY
	else if (cut % MODULUS_FREEROAM_OR_RACE === 0 && cut % MODULUS_FREEROAM_ONLY !== 0)
	{
		// ----------------------- h雷 var cut-lista f他ut!

		mesh_checkpoint_finish.visible = true;
